{
    "logger": {
        "applicationName": "api-gateway",
        "excludePaths": ["/ping", "/metrics"],
        "console": {
            "enabled": true
        },
        "file": {
            "enabled": true,
            "path": "logs/logs.txt",
            "interval": "day"
        },
        "seq": {
            "enabled": true,
            "url": "http://localhost:5341",
            "token": "secret"
        }
    },
    "metrics": {
        "enabled": true,
        "influxEnabled": false,
        "prometheusEnabled": true,
        "influxUrl": "http://localhost:8086",
        "database": "pacco",
        "env": "local",
        "interval": 5
    }
}